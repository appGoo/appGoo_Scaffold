{
    "appBuild": {
        "includeFileQualifier": ".include",
        "sqlFileQualifier": ".sql",
        "modifiedOnly": "no",
        "processSQL": "per-file/batch",
        "build-01": "source/sql",
        "build-02": "source/sql/play"
        },
    "appDataUpgrade": {
        "includeFileQualifier": ".include",
        "sqlFileQualifier": ".sql",
        "upgrade-01": "source/install/upgrade"
        },
    "preprocess": {
        "do-preprocess": "no/yes/if-installed",
        "script-01": "preprocess.sh"
        },
    "postprocess": {
        "do-postprocess": "no/yes",
        "script-01": "postprocess.sh"
        },    
    "runOptions": {
        "buildFileExt": ".build",
        "appGooFileExt": ".agsql",
        "sqlFileExt": ".sql",
        "dbUser": "postgres",
        "sqlCmd": "psql &CMDS --dbname=&DB --username=&UNAME --no-password",
        "fileLog": "yes",
        "dbLog": "no"
    },
    "agAbout": {
        "CreatedBy": "Install",
        "Purpose": "This is the default config file, change this text to your reason why this file exists"
    }
}
